{
  "name": "avesta-admin-panel",
  "version": "1.0.0",
  "description": "Admin Panel",
  "main": "app.ts",
  "scripts": {
    "test": "jest",
    "build": "node_modules/typescript/bin/tsc",
    "dev": "cross-env NODE_ENV=development ts-node-dev --transpile-only --project ./tsconfig.json --respawn  src/adminPanel/presentation/app.ts",
    "migrate:dev": "npm run build && cross-env NODE_ENV=development dotenv -e .env sequelize db:migrate",
    "migrate:dev:undo": "npm run build && cross-env NODE_ENV=development dotenv -e .env sequelize db:migrate:undo"
  },
  "author": "Vishvjit",
  "license": "ISC",
  "dependencies": {
    "@sendgrid/mail": "^8.1.0",
    "axios": "^1.6.7",
    "body-parser": "^1.20.2",
    "dotenv": "^16.4.1",
    "dotenv-cli": "^7.3.0",
    "duckdb": "^0.9.2",
    "duckdb-async": "^0.9.2",
    "express": "^4.18.2",
    "express-joi-validation": "^5.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.7.1",
    "nodemailer": "^6.9.8",
    "otp-generator": "^4.0.1",
    "path": "^0.12.7",
    "redis": "^4.6.12"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.11.5",
    "@types/nodemailer": "^6.4.14",

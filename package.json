    "@types/sequelize": "^4.28.20",
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "sequelize": "^6.35.2",
    "sequelize-cli": "^6.6.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  }
}
